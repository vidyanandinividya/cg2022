package session1;

import java.util.Scanner;

public class LoopDemo {

	public static void main(String[] args) {
		//that prints the sum of 5 numbers
		Scanner sc=new Scanner(System.in);
		int num=1;
		int sum=0;
		while(num<=5)
		{
			sum+=num;
			num++;
		}
		System.out.println("The sum is:"+sum);

	}

}
